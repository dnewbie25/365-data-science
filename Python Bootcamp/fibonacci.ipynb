    "def fibonacci(position):\n",
    "  if position < 1:\n",
    "    return \"Doesn't work for numbers smaller than 1\"\n",
    "  elif 1 <= position <= 2:\n",
    "    return 1\n",
    "  return fibonacci(position-1) + fibonacci(position-2)\n",
    "\n",
    "print(fibonacci(30))"
   ]
  }
